        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended",
        "plugin:cypress/recommended"],
    "plugins": ["cypress"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-unused-vars": "warn"
    }
}
