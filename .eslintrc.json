{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:cypress/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["cypress", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
